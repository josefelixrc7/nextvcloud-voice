{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Vicibox Server (VirtualBox)",
            "type": "shell",
            "command": "VBoxManage startvm \"ViciBox Server\" --type headless"
        }
        ,{
            "label": "Poweroff Vicibox Server (VirtualBox)",
            "type": "shell",
            "command": "VBoxManage controlvm \"ViciBox Server\" poweroff"
        }
    ]
}